#!/bin/bash

pushd ${HOME}/.emacs.src.d

git clone git://orgmode.org/org-mode.git
pushd org-mode
git checkout maint
git checkout -b local
make compile
make autoloads
touch README
popd

git clone http://git.gnus.org/gnus.git
pushd gnus
git checkout -b local
./configure && make
popd

git clone https://github.com/fujii/Sublain.git
pushd Sublain
git checkout -b local
popd

git clone https://github.com/dengste/org-caldav.git
pushd org-caldav
git checkout -b local
popd

popd
